	<queryString>
		<![CDATA[SELECT ID_INSTRUCCION_ASUNTO, INS_INSTRUCCION, 
(SELECT INS_NOMBRE FROM SD_INSTRUCCION WHERE ID_INSTRUCCION=INS_INSTRUCCION) AS INS_INSNOMBRE,
CASE ASU_ESTATUS 
	WHEN 0 THEN 'En Recepción' 
	WHEN 1 THEN 'Por Turnar' 
	WHEN 2 THEN 'En Tramite' 
	WHEN 3 THEN 'Terminada' 
END AS ESTATUS,
TO_CHAR(INS_FECHA_ASIGNACION, 'yyyy-MM-dd') AS FECHA_ASIGNACION, 
TO_CHAR(INS_FECHA_TERMINACION, 'yyyy-MM-dd') AS FECHA_TERMINACION, 
INS_ESTATUS, ASU_FOLIO_RECEPCION, ASU_DESCRIPCION, 
INS_INSTRUCCION_OTRO, INS_TIEMPO, INS_AVANCE, INS_USUARIO, 
USU_ASISTENTE, USU_JEFE, INS_FECHA_ASIGNACION 
FROM SD_INSTRUCCION_ASUNTO, SD_USUARIO, 
SD_ASUNTO WHERE INS_USUARIO=$P{ID_REMITENTE} AND 
SD_ASUNTO.ID_ASUNTO=SD_INSTRUCCION_ASUNTO.ID_ASUNTO AND 
SD_USUARIO.ID_USUARIO=SD_INSTRUCCION_ASUNTO.INS_USUARIO 
AND INS_ESTATUS=3 AND INS_BANDERA1=0 
ORDER BY ASU_FOLIO_RECEPCION ASC]]>
	</queryString>
