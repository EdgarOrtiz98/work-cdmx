<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE taglib PUBLIC
	"-//Sun Microsystems, Inc.//DTD JSP Tag Library 1.1//EN"
	"http://java.sun.com/j2ee/dtds/web-jsptaglibrary_1_1/dtd">
<taglib>

	<tlibversion>1.0</tlibversion>
	<jspversion>1.1</jspversion>
	<shortname>plib</shortname>
	
	<info>Libreria de Tags de Meve</info>
	
	<tag>
		<name>asuntos</name>
		<tagclass>com.meve.sigma.taglib.TglArbolInstrucciones</tagclass>
		<bodycontent>empty</bodycontent>
		<info>Crea un script con el Arbol de Asuntos.</info>		
		
		<attribute>
		    <name>id_asunto</name>
		    <required>true</required>
		    <rtexprvalue>true</rtexprvalue>
		</attribute>

		<attribute>
			<name>nombre</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>

		<attribute>
			<name>filtro</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>

</taglib>
