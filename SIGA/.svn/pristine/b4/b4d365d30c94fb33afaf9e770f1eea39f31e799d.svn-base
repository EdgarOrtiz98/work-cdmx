<?xml version="1.0" encoding="ISO-8859-1" ?>
<!DOCTYPE struts-config PUBLIC
          "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN"
          "http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">

<struts-config>

	<form-beans>
		<form-bean name="asuntoForm"
			type="com.meve.sigma.asunto.AsuntoForm" />
		<form-bean name="directorioForm"
			type="com.meve.sigma.asunto.AsuntoForm" />
		<form-bean name="remitenteForm"
			type="com.meve.sigma.asunto.AsuntoForm" />
		<form-bean name="catalogoEntidad"
			type="com.meve.sigma.catalogos.CatalogoEntidad" />
		<form-bean name="catalogoRemitente"
			type="com.meve.sigma.catalogos.CatalogoRemitente" />
		<form-bean name="reporteForm"
			type="com.meve.sigma.reporte.ReporteForm" />
		<form-bean name="parametrosForm"
			type="com.meve.sigma.parametros.ParametrosForm" />
		<form-bean name="estructuraOrganizacionalForm"
			type="com.meve.sigma.organizacional.EstructuraOrganizacionalForm" />
		<form-bean name="puestoForm"
			type="com.meve.sigma.organizacional.puesto.PuestoForm" />
	</form-beans>


	<global-forwards>
		<forward name="captura" path="/jsp/captura.jsp" />
		<forward name="directorio" path="/jsp/directorioAreas.jsp" />
		<forward name="directorioExternas" path="/jsp/directorioAreas.jsp?idLink=cargaExternos" />
		<forward name="nuevaEntidad" path="/jsp/nuevaEntidad.jsp" />
		<forward name="remitentes" path="/jsp/remitentes.jsp"/>
		<forward name="nuevoRemitente" path="/jsp/nuevoRemitente.jsp" />
		<forward name="submit" path="/jsp/cargandoSubmit.jsp" />
		<forward name="nuevoParametro" path="/jsp/insertarParametros.jsp" />
		<forward name="reporteador" path="/jsp/reporteador.jsp" />
		<forward name="mostrarParametros" path="/jsp/mostrarParametros.jsp" />
		<forward name="modificarParametro" path="/jsp/modificarParametros.jsp" />
		<forward name="estructuraOrganizacional" path="/jsp/estructuraOrganizacional.jsp" />
		<forward name="puesto" path="/jsp/puesto.jsp" />
		<forward name="puestoEdicion" path="/jsp/puestoEdicion.jsp" />
		<forward name="estructuraOrganizacionalPorRoles" path="/jsp/estructuraOrganizacionalPorRoles.jsp" />
	</global-forwards>

	<action-mappings>
		
		<action path="/captura"
			type="com.meve.sigma.asunto.AsuntoAction" name="asuntoForm"
			input="/jsp/captura.jsp" validate="true">
		</action>
		<action path="/directorio"
			type="com.meve.sigma.asunto.AsuntoAction" name="directorioForm"
			input="/jsp/directorioAreas.jsp" validate="true">
		</action>
		<action path="/remitente"
			type="com.meve.sigma.asunto.AsuntoAction" name="remitenteForm"
			input="/jsp/remitentes.jsp" validate="true">
		</action>
		<action path="/nuevaEntidad"
			type="com.meve.sigma.catalogos.CatalogoAction" name="catalogoEntidad"
			input="/jsp/nuevaEntidad.jsp" validate="true">
		</action>
	    <action path="/nuevoRemitente"
			type="com.meve.sigma.catalogos.CatalogoAction" name="catalogoRemitente"
			input="/jsp/nuevoRemitente.jsp" validate="true">
		</action>
	    <action path="/reporteador"
			type="com.meve.sigma.reporte.ReporteAction" name="reporteForm"
			input="/jsp/reporteador.jsp" validate="true">
		</action>
	    <action path="/parametros"
			type="com.meve.sigma.parametros.ParametrosAction" name="parametrosForm"
			input="/jsp/insertarParametros.jsp" validate="true">
		</action>
	    <action path="/estructuraOrganizacional"
			type="com.meve.sigma.organizacional.EstructuraOrganizacionalAction" name="estructuraOrganizacionalForm"
			input="/jsp/estructuraOrganizacional.jsp" validate="true">
		</action>
		<action path="/puesto"
			type="com.meve.sigma.organizacional.puesto.PuestoAction" name="puestoForm"
			input="/jsp/puesto.jsp" validate="true">
		</action>
	</action-mappings>

	<message-resources parameter="prop_es" />
	<plug-in className="com.meve.sigma.plugins.AppServerPlugin" />

</struts-config>
