	<queryString>
		<![CDATA[SELECT ID_ASUNTO, ASU_DESCRIPCION, 
CASE ASU_ESTATUS WHEN 1 THEN 'Por Turnar' END AS ESTATUS,
ASU_FOLIO_RECEPCION,
ASU_AVANCE, USU_AREA, PRI_NUMERO_HEX, ASU_USUARIO_CAPTURA,
to_char(ASU_FECHA_CAPTURA, 'dd/MM/yyyy') AS FECHA_CAPTURA, ASU_FOLIO_EXTERNO
FROM SD_ASUNTO LEFT OUTER JOIN SD_PRIORIDAD
ON SD_ASUNTO.ASU_PRIORIDAD=SD_PRIORIDAD.ID_PRIORIDADES, SD_USUARIO
WHERE ASU_ESTATUS=1
AND SD_USUARIO.ID_USUARIO=SD_ASUNTO.ASU_USR_TURNA
AND ASU_USR_TURNA=$P{ID_USUARIO}
ORDER BY LOWER(ASU_DESCRIPCION)]]>
	</queryString>
