package com.meve.sigma.beans;

public class ConfiguracionBean {
	
	private int idConfiguracion;
	private int duracion;
	private int semaforoPorcentajeAmarillo;
	private int paginacionRegistrosPorPagina;
	private int paginacionPaginasMostrables;

	public int getIdConfiguracion() {
		return idConfiguracion;
	}

	public void setIdConfiguracion(int idConfiguracion) {
		this.idConfiguracion = idConfiguracion;
	}

	public int getDuracion() {
		return duracion;
	}

	public void setDuracion(int duracion) {
		this.duracion = duracion;
	}

	public int getSemaforoPorcentajeAmarillo() {
		return semaforoPorcentajeAmarillo;
	}

	public void setSemaforoPorcentajeAmarillo(int semaforoPorcentajeAmarillo) {
		this.semaforoPorcentajeAmarillo = semaforoPorcentajeAmarillo;
	}

	public int getPaginacionRegistrosPorPagina() {
		return paginacionRegistrosPorPagina;
	}

	public void setPaginacionRegistrosPorPagina(int paginacionRegistrosPorPagina) {
		this.paginacionRegistrosPorPagina = paginacionRegistrosPorPagina;
	}

	public int getPaginacionPaginasMostrables() {
		return paginacionPaginasMostrables;
	}

	public void setPaginacionPaginasMostrables(int paginacionPaginasMostrables) {
		this.paginacionPaginasMostrables = paginacionPaginasMostrables;
	}

}
