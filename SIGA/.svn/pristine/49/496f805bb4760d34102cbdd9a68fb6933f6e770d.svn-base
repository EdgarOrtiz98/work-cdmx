package com.meve.sigma.catalogos;

import java.util.List;

public class CatalogoUtil {

	public static CatalogoGenerico getCatalogo(List catalogo, int id) {
		for (int i = 0; i < catalogo.size(); i++) {
			if (((CatalogoGenerico) catalogo.get(i)).getId() == id) {
				return (CatalogoGenerico) catalogo.get(i);
			}
		}
		return null;

	}

}
