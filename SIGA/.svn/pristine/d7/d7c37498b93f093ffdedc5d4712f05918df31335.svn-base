package com.meve.sigma.beans;

/**
 * Bean que almacena los datos de un ciudadano al registrarse
 * 
 * @author Meve Soluciones SA de CV
 */
public class BeanCiudadano {
    
	/**
	 * Id usuario
	 */
    private String strIdUsuario;
    
    /**
     * Nombre ciudadano
     */
    private String strNombre;
    
    /**
     * Apellido paterno
     */
    private String strApellidoPaterno;
    
    /**
     * Apellido materno
     */
    private String strApellidoMaterno;
    
    /**
     * Mail
     */
    private String strMail;
    
    /**
     * usuario
     */
    private String strUsuario;
    
    /**
     * Ocupación
     */
    private String strOcupacion;
    
    /**
     * Domicilio
     */
    private String strDomicilio;
    
    /**
     * País
     */
    private String strPais;
    
    /**
     * Estado
     */
    private String strEstado;
    
    /**
     * Municipio
     */
    private String strMunicipio;
    
    /**
     * Teléfono
     */
    private String strTelefono;
    
    
    /**
     * Sector
     */
    private String strSector;
    
    /**
     * Sexo
     */
    private String strSexo;
    
    /**
     * Fecha necimiento
     */
    private String strFechaNac;
    
    /**
     * Bean que carga los datos del ciudadano
     * @param idUsuario	id usuario
     * @param nombre	Nombre ciudadano
     * @param apellidoP	Apellido Paterno
     * @param apellidoM	Apellido materno
     * @param mail		Mail
     * @param usuario	usuario
     * @param ocupacion	Ocupación
     * @param domicilio	Domicilio
     * @param pais		País
     * @param estado	Estado
     * @param municipio	Municipio
     * @param telefono	Teléfono
     * @param sector	Sector
     * @param sexo		Sexo
     * @param fechaNac	Fecha Nacimiento
     */
    public BeanCiudadano(String idUsuario, String nombre, String apellidoP, 
            				String apellidoM, String mail, String usuario, 
            				String ocupacion, String domicilio, String pais,
            				String estado, String municipio, String telefono,
            				String sector, String sexo, String fechaNac){
        strIdUsuario = idUsuario;
        strNombre = nombre;
        strApellidoPaterno = apellidoP;
        strApellidoMaterno = apellidoM;
        strMail	= mail;
        strUsuario = usuario;
        strOcupacion = ocupacion;
        strDomicilio = domicilio;
        strPais = pais;
        strEstado = estado;
        strMunicipio = municipio;
        strTelefono = telefono;
        strSector = sector;
        strSexo = sexo;
        strFechaNac = fechaNac;
    }
    
    /**
     *	Contructor
     */
    public BeanCiudadano(){}
    
    /**
     * @return	Devuelve el id usuario
     */
    public String getIdUsuario(){
        return strIdUsuario;
    }
    
    /**
     * @return	devuelve el nombre
     */
    public String getNombre(){
        return strNombre;
    }
    
    /**
     * @return	Devuelve el apellido paterno
     */
    public String getApellidoPaterno(){
        return strApellidoPaterno;
    }
    
    /**
     * @return	Devuelve el apellido materno
     */
    public String getApellidoMaterno(){
        return strApellidoMaterno;
    }
    
    /**
     * Devuelve el mail
     * @return mail
     */
    public String getMail(){
        return strMail;
    }
    
    /**
     * Devuelve el usuario
     * @return usuario
     */
    public String getUsuario(){
        return strUsuario;
    }
    
    /**
     * Devuelve la Ocupación
     * @return Ocupación
     */
    public String getOcupacion(){
        return strOcupacion;
    }
    
    /**
     * Devuelve el domicilio
     * @return domicilio
     */
    public String getDomicilio(){
        return strDomicilio;
    }
    
    /**
     * Devuelve el País
     * @return País
     */
    public String getPais(){
        return strPais;
    }
    
    /**
     * Devuelve el estado
     * @return estado
     */
    public String getEstado(){
        return strEstado;
    }
    
    /**
     * Devuelve el municipio
     * @return Municipio
     */
    public String getMunicipio(){
        return strMunicipio;
    }
    
    /**
     * Devuelve el Teléfono
     * @return Teléfono
     */
    public String getTelefono(){
        return strTelefono;
    }
    
    /**
     * Devuelve el sector
     * @return sector
     */
    public String getSector(){
        return strSector;
    }

}

