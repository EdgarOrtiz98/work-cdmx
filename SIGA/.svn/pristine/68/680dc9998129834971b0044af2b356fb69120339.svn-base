	<parameter name="ARG_BUSCAR" class="java.lang.String" isForPrompting="false">
		<defaultValueExpression><![CDATA[" SELECT ASU_FOLIO_RECEPCION, ASU_REMITENTE, ASU_DESCRIPCION, TO_CHAR(ASU_FECHA_CAPTURA, 'DD/MM/YYYY') AS FECHA, " + 
" ID_ASUNTO, ASU_CONTROL, ASU_FECHA_CAPTURA,  " +
" CASE ASU_ESTATUS  " +
" WHEN 0 THEN (SELECT USU_CARGO FROM SD_USUARIO WHERE ID_USUARIO=ASU_REMITENTE)  " +
" WHEN 1 THEN (SELECT USU_CARGO FROM SD_USUARIO WHERE ID_USUARIO=ASU_REMITENTE)  " +
" WHEN 2 THEN (SELECT ASU_REF_REMITENTE_NOMBRE FROM SD_ASUNTO_DESCRIPCION WHERE ID_ASUNTO_REF=ID_ASUNTO)  " +
" WHEN 3 THEN (SELECT ASU_REF_REMITENTE_NOMBRE FROM SD_ASUNTO_DESCRIPCION WHERE ID_ASUNTO_REF=ID_ASUNTO)  " +
" END AS USUARIO,  " +
" CASE ASU_ESTATUS  " +
" WHEN 0 THEN (SELECT RMT_NOMBRE FROM SD_REMITENTE WHERE ID_REMITENTE=ASU_REMITENTE)  " +
" WHEN 1 THEN (SELECT RMT_NOMBRE FROM SD_REMITENTE WHERE ID_REMITENTE=ASU_REMITENTE)  " +
" WHEN 2 THEN (SELECT ASU_REF_REMITENTE_NOMBRE FROM SD_ASUNTO_DESCRIPCION WHERE ID_ASUNTO_REF=ID_ASUNTO)  " +
" WHEN 3 THEN (SELECT ASU_REF_REMITENTE_NOMBRE FROM SD_ASUNTO_DESCRIPCION WHERE ID_ASUNTO_REF=ID_ASUNTO)  " +
" END AS REMITENTE  " +
" FROM SD_ASUNTO  " +
" WHERE ID_ASUNTO IN ( SELECT ID_ASUNTO FROM SD_ASUNTO, SD_USUARIO, SD_USUARIO_ROL  " +
" WHERE SD_USUARIO.ID_USUARIO=SD_ASUNTO.ASU_USR_TURNA AND SD_USUARIO.ID_USUARIO = SD_USUARIO_ROL.ID_USUARIO1  " +
" AND (ASU_REMITENTE=25 OR ASU_USUARIO_CAPTURA= 25 OR ASU_USR_TURNA= 25 OR (USU_AREA=16 AND ID_ROL=5)  " +
" OR (USU_AREA=16 AND ID_ROL=4) ) UNION SELECT ID_ASUNTO FROM SD_INSTRUCCION_ASUNTO WHERE INS_USUARIO = 25  " +
" AND INS_ACCESO = 1) AND ASU_ESTATUS IN (0,1,2,3) ORDER BY 1 asc, 1 ASC    "]]></defaultValueExpression>
	</parameter>
