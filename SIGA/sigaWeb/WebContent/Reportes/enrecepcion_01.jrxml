	<queryString>
		<![CDATA[SELECT ID_ASUNTO, ASU_DESCRIPCION, ASU_ESTATUS, ASU_FOLIO_RECEPCION, 
CASE ASU_ESTATUS WHEN 0 THEN 'En Recepción' END AS ESTATUS,
ASU_AVANCE, USU_AREA, PRI_NUMERO_HEX, ASU_USUARIO_CAPTURA, 
TO_CHAR(ASU_FECHA_CAPTURA, 'DD/MM/YYYY') AS FECHA, ASU_FOLIO_EXTERNO 
FROM SD_ASUNTO LEFT OUTER JOIN SD_PRIORIDAD 
ON SD_ASUNTO.ASU_PRIORIDAD=SD_PRIORIDAD.ID_PRIORIDADES, SD_USUARIO 
WHERE ASU_ESTATUS=0 AND SD_USUARIO.ID_USUARIO=SD_ASUNTO.ASU_USR_TURNA 
AND USU_AREA = $P{USU_AREA} AND ASU_USUARIO_CAPTURA = $P{ID_USUARIO}
ORDER BY LOWER(ASU_DESCRIPCION)]]>
	</queryString>
