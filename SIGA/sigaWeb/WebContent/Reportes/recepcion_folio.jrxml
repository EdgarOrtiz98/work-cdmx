	<queryString>
		<![CDATA[SELECT ID_ASUNTO, ASU_FOLIO_RECEPCION, ASU_DESCRIPCION, ASU_REMITENTE, ASU_ESTATUS,
to_char(ASU_FECHA_CAPTURA, 'DD/MM/YYYY') ASU_FECHA_CAPTURA_CHAR, USU_AREA, ASU_CONTROL,
CASE ASU_CONTROL
         WHEN 1 THEN (SELECT USU_CARGO FROM SD_USUARIO WHERE ID_USUARIO=ASU_REMITENTE)
         WHEN 0 THEN (SELECT RMT_NOMBRE FROM SD_REMITENTE WHERE ID_REMITENTE=ASU_REMITENTE)
      END AS NOMBRE 
FROM SD_ASUNTO, SD_USUARIO WHERE ASU_ESTATUS=1 
AND SD_USUARIO.ID_USUARIO=SD_ASUNTO.ASU_USR_TURNA 
AND USU_AREA=$P{USU_AREA}
ORDER BY 2]]>
