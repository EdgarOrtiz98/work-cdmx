	<queryString>
		<![CDATA[SELECT ID_ASUNTO, ASU_DESCRIPCION, ASU_ESTATUS, ASU_FOLIO_RECEPCION,
CASE ASU_ESTATUS WHEN 2 THEN 'En Trámite' END AS ESTATUS, 
ASU_AVANCE, USU_AREA, PRI_NUMERO_HEX, TO_CHAR(ASU_FECHA_CADUCIDAD, 'dd/MM/yyyy') AS FECHA_CADUCIDAD,
TO_CHAR(ASU_FECHA_CAPTURA, 'dd/MM/yyyy') AS FECHA_CAPUTRA, ASU_FOLIO_EXTERNO
FROM SD_ASUNTO LEFT OUTER JOIN SD_PRIORIDAD
ON SD_ASUNTO.ASU_PRIORIDAD=SD_PRIORIDAD.ID_PRIORIDADES, SD_USUARIO
WHERE ASU_ESTATUS=2
AND SD_USUARIO.ID_USUARIO=SD_ASUNTO.ASU_USR_TURNA
AND ASU_USR_TURNA=$P{ID_USUARIO}
ORDER BY LOWER(ASU_DESCRIPCION)]]>
	</queryString>
