	<parameter name="ID_ASUNTO" class="java.math.BigDecimal" isForPrompting="false"/>
	<queryString>
		<![CDATA[SELECT a1.id_asuntos_rel, 
CASE a1.tipo_ref WHEN 'a' THEN 'Alcance' WHEN 'f' THEN 'Referencia' WHEN 'r' THEN 'Respuesta' END AS TIPO, 
a2.ASU_FOLIO_RECEPCION, a2.ASU_FOLIO_EXTERNO, 
CASE a2.ASU_ESTATUS WHEN 2 THEN 'Tramite' WHEN 3 THEN 'Terminado' END AS ESTATUS
from SD_ASUNTOS__ANT a1, SD_ASUNTO a2, SD_USUARIO a3
WHERE a1.id_asuntos_rel=a2.ID_ASUNTO AND
a3.ID_USUARIO=a2.ASU_USR_TURNA AND
(a2.ASU_ESTATUS = 3 OR a2.ASU_ESTATUS = 2) AND
a1.ID_ASUNTO=$P{ID_ASUNTO} ORDER BY 1]]>
	</queryString>
