	<queryString>
		<![CDATA[SELECT ID_ASUNTO, ASU_DESCRIPCION, ASU_FOLIO_RECEPCION, 
CASE ASU_ESTATUS 
	WHEN 0 THEN 'En Recepción' 
	WHEN 1 THEN 'Por Turnar' 
	WHEN 2 THEN 'En Tramite' 
	WHEN 3 THEN 'Terminada' 
END AS ESTATUS,
ASU_AVANCE, ASU_FOLIO_EXTERNO, ASU_ESTATUS, USU_CARGO 
FROM SD_ASUNTO, SD_USUARIO WHERE ASU_REMITENTE=$P{ID_USUARIO}
AND SD_USUARIO.ID_USUARIO=SD_ASUNTO.ASU_USR_TURNA AND ASU_CONTROL=1
ORDER BY LOWER(ASU_DESCRIPCION) ASC]]>
	</queryString>
